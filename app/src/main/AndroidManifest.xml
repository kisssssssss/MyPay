<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/app_wallet"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_wallet"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyPay"
        tools:targetApi="31">
        <activity
            android:name="com.kis.mypay.AuthorityActivity.AuthorityActivity"
            android:exported="true"
            android:theme="@style/Theme.MyPay"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.kis.mypay.MainActivity.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.MyPay"></activity>
    </application>

</manifest>